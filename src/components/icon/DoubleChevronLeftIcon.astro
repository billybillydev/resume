---
import { tailwindThemeConfig } from "@config/design";
import SVG from "@components/svg/SVG.astro";
import type { IconProps } from "@components/icon/props";

type Props = IconProps;

const { size = 4, ...restProps } = Astro.props;
---

<SVG viewBox="0 0 24 24" style={`width: ${tailwindThemeConfig.width[size]}; height: ${tailwindThemeConfig.height[size]};`} {...restProps}>
  <path stroke-linecap="round" stroke-linejoin="round" d="M18.75 19.5l-7.5-7.5 7.5-7.5m-6 15L5.25 12l7.5-7.5" />
</SVG>

